{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "plugins": ["prettier"],
  "rules": {
    "no-undef": "error",
    "no-console": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": false,
        "trailingComma": "es5"
      }
    ]
  },
  "env": {
    "es6": true,
    "node": true
  }
}
